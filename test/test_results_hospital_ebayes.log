: real matrix vectorToStripeDiag(real vector m) {
>     // Add debugging
>     printf("Input vector m dimensions: %f x %f\n", rows(m), cols(m))
>     
>     real scalar dim
>     dim = length(m)
> 
>     // pre-allocate matrix M
>     real matrix M
>     M=J(dim,dim,.)
> 
>     // fill lower triangle of M
>     real scalar i
>     real scalar j
>     for (i=1; i<=dim; i++) {
>         for (j=i; j<=dim; j++) {
>             M[j,i]=m[j-i+1]
>         }
>     }
> 
>     _makesymmetric(M)
>     return (M)
> }

