> 
>     // pre-allocate matrix
>     real matrix CC
>     CC = J(dim,4,.)
> 
>     // Fill cov's and corr's: between time t and t+i
>     real scalar i
>     real scalar tstat
>     for (i=1; i<=dim; i++) {
>         // check that there are >=2 obs, in order to compute covariance
>         stata(invtokens(("quietly count if !missing(",scores_var,",f",strofreal(i),".",scores_var,")"),""))
>         if (st_numscalar("r(N)")>1) {
