>             CC[i,1]=st_numscalar("r(cov_12)")
>             CC[i,2]=CC[i,1] / ( sqrt(st_numscalar("r(Var_1)")) * sqrt(st_numscalar("r(Var_2)")) )
>         }
>         CC[i,3]=st_numscalar("r(N)")
> 
