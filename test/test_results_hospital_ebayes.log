: // Helper function to parse leaveout rules
: void _parse_rule(string scalar rule, string scalar before, string scalar after) {
>     // Input: rule = "-2,2"
>     string vector parts
>     parts = tokens(rule, ",")  // Split at comma: ("-2", "2")
>     before = parts[1]          // before = "-2"
