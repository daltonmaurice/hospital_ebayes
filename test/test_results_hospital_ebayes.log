128.     if "`shrinkage_target'" != "" {
129.         qui by `hospitalid' `year' `class': egen `mshrinktarget' = mean(y_shrinktarget)
130.     }
131. 
.     tempname var_total
132.     qui sum score_r
133.     /// from looking I think this might var(A_it)
>     scalar `var_total' = r(Var)*((`num_obs' - 1)/(`num_obs' - `num_par'))
