. 
. if ("`leaveout_years'"!="") {
 21.     // Parse the leaveout rules
.     local n_rules = 0
 22.     foreach rule in `leaveout_years' {
 23.         local ++n_rules
 24.         tokenize "`rule'", parse(",")
 25.         local rule_`n_rules'_before "`1'"
 26.         local rule_`n_rules'_after "`3'"
 27.     }
 28.     
